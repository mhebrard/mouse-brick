{
  "name": "mouse-brick",
  "productName": "Mouse BrICK",
  "version": "1.0.0",
  "description": "Mouse BReeding Interfaced Calendar Kit is a project of desctop app to help managment of mice breeding.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=packs",
    "pack-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=packs --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Mouse BrICK\"",
    "pack-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=packs",
    "test": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhebrard/mouse-brick.git"
  },
  "keywords": [],
  "author": "Maxime Hebrard <maxime.hebrard@gmail.com> (http://github.com/mhebrard)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mhebrard/mouse-brick/issues"
  },
  "homepage": "https://github.com/mhebrard/mouse-brick#readme",
  "dependencies": {
    "electron": "^1.7.6",
    "graceful-fs": "^4.1.11",
    "sql.js": "^0.4.0",
    "vis": "^4.20.1"
  },
  "devDependencies": {
    "electron": "^1.7.6",
    "electron-packager": "^9.0.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": 2,
    "env": [
      "browser",
      "node"
    ]
  }
}
